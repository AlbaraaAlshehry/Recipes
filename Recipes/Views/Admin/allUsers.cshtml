@model IEnumerable<UserLogin>
@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table">
    <thead>
        <tr>
            <th>Full Name </th>
            <th>User Name </th>
            <th>Email</th>
            <th>Role</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.User.Fname @item.User.Lname</td>
                <td>@item.UserName</td>
                <td>@item.User.Email</td>
                <td>@item.Role.RoleName</td>
                
            </tr>
        }

    </tbody>
</table>